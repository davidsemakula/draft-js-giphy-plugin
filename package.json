{
  "name": "draft-js-giphy-plugin",
  "version": "0.1.0-rc.18",
  "description": "Giphy Plugin for DraftJS",
  "author": {
    "name": "David Semakula",
    "email": "tdsemakula@gmail.com",
    "url": "http://davidsemakula.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidsemakula/draft-js-giphy-plugin.git"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/rimraf lib",
    "build": "yarn clean && yarn build:js && yarn build:css",
    "build:js": "./node_modules/.bin/rollup --config ./rollup.config.js",
    "build:css": "node ./scripts/build-css.js $(pwd)",
    "prepublish": "yarn build"
  },
  "license": "MIT",
  "dependencies": {
    "@giphy/js-fetch-api": "^1.5.9",
    "@giphy/react-components": "^1.0.5",
    "immutable": "~3.7.4",
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1 || ^0.11.0",
    "react": "^15.5.0 || ^16.0.0-rc",
    "react-dom": "^15.5.0 || ^16.0.0-rc"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^6.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "linaria": "^1.4.0-alpha.1",
    "matched": "^4.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
